{
    "type": "object",
    "properties": {
        "first_name": {
            "type": "string",
            "pattern": "^[a-zA-Z]*$"
        },
        "last_name": {
            "type": "string",
            "pattern": "^[a-zA-Z]*$"
        },
        "password": {
            "type": "string",
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d).{8,}$"
        }
    },
    "required": ["first_name", "last_name", "password"],
    "additionalProperties": false
}
